<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:aigestudio="http://schemas.android.com/apk/res-auto"
    tools:context="makejin.langfriend.juwon.Signup.Signup6AddImageFragment">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        android:id="@+id/appBarLayout">
        
        <include layout="@layout/toolbar_default" android:id="@+id/cs_toolbar" />

    </android.support.design.widget.AppBarLayout>

    <RelativeLayout
        android:id="@+id/RL_BT_yes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp">
        <Button
            android:id="@+id/BT_yes"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="다음"
            android:textColor="#000000"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/RL_guide"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="15dp"
        android:paddingBottom="-20dp"
        android:layout_below="@+id/appBarLayout"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사진을 추가해주세요"
            android:textSize="17dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="100dp"
        android:layout_marginBottom="70dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingTop="10dp">

        <RelativeLayout
            android:id="@+id/RL_IV_pic1"
            android:layout_width="230dp"
            android:layout_height="250dp">
            <ImageView
                android:id="@+id/IV_pic1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_1"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/RL_IV_pic6"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="105dp"
            android:layout_height="115dp"
            android:layout_below="@id/RL_IV_pic1">
            <ImageView
                android:id="@+id/IV_pic6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_6"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_6"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_6"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator6"/>
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/RL_IV_pic5"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="105dp"
            android:layout_height="115dp"
            android:layout_below="@id/RL_IV_pic1"
            android:layout_toRightOf="@id/RL_IV_pic6">
            <ImageView
                android:id="@+id/IV_pic5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_5"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_5"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator5"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RL_IV_pic4"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="105dp"
            android:layout_height="115dp"
            android:layout_below="@id/RL_IV_pic1"
            android:layout_toRightOf="@id/RL_IV_pic5">
            <ImageView
                android:id="@+id/IV_pic4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_4"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_4"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator4"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RL_IV_pic3"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_width="105dp"
            android:layout_height="115dp"
            android:layout_above="@id/RL_IV_pic4"
            android:layout_toRightOf="@id/RL_IV_pic1">
            <ImageView
                android:id="@+id/IV_pic3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_3"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_3"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator3"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RL_IV_pic2"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="10dp"
            android:layout_width="105dp"
            android:layout_height="115dp"
            android:layout_above="@id/RL_IV_pic3"
            android:layout_toRightOf="@id/RL_IV_pic1">
            <ImageView
                android:id="@+id/IV_pic2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#AAAAAA"
                android:scaleType="fitXY"/>
            <RelativeLayout
                android:id="@+id/RL_RL_no_2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="-10dp"
                android:layout_marginRight="-10dp"
                android:visibility="invisible">
                <RelativeLayout
                    android:id="@+id/RL_no_2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/border_round_no"
                    android:layout_centerInParent="true">
                    <Button
                        android:id="@+id/BT_no_2"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:background="@drawable/mark_no"
                        android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <include layout="@layout/view_loading_indicator" android:id="@+id/indicator2"/>
        </RelativeLayout>

    </RelativeLayout>



    <include layout="@layout/view_loading_indicator" android:id="@+id/indicator"/>

</RelativeLayout>
